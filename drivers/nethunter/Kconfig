menu "NetHunter Support"

config NETHUNTER_SUPPORT
	bool "Basic nethunter support"
	default n
	help
		Enable basic nethunter support in-kernel which are sysvipc, module support, hci0 support and mac80211 support.

	select MODULES
	select MODULE_FORCE_LOAD
	select MODULE_UNLOAD
	select MODVERSIONS
	
	select SYSVIPC
	select SYSVIPC_SYSCTL
	select POSIX_MQUEUE
	
	select IKCONFIG
	select CPUSETS
	select AUTOFS4_FS
	select TMPFS_XATTR
	select TMPFS_POSIX_ACL
	select CGROUPS
	select CGROUP_DEVICE
	
	select NAMESPACES
	select UTS_NS
	select IPC_NS
	select USER_NS
	select PID_NS
	select NET_NS
	
	select DEVTMPFS
	select DEVTMPFS_MOUNT
	select FSNOTIFY
	select DNOTIFY
	select INOTIFY_USER
	select FANOTIFY
	select FANOTIFY_ACCESS_PERMISSIONS
	select MMC
	select FHANDLE
	select EPOLL
	select SIGNALFD
	select TIMERFD
	
	select BT
	select BT_BREDR
	select BT_RFCOMM
	select BT_RFCOMM_TTY
	select BT_BNEP
	select BT_BNEP_MC_FILTER
	select BT_BNEP_PROTO_FILTER
	select BT_HIDP
	select BT_HS
	select BT_LE
	select BT_LEDS
	select BT_HCIBFUSB
	select BT_HCIBTSDIO
	select BT_HCIBTUSB
	select BT_HCIBTUSB_BCM
	select BT_HCIBTUSB_RTL
	select BT_HCIUART
	select BT_HCIVHCI
	select BT_HCIBCM203X
	select BT_HCIBPA10X

config NETHUNTER_ETHERNET_SUPPORT
	tristate "Ethernet drivers support"
	default y if NETHUNTER_SUPPORT
	depends on NETHUNTER_SUPPORT
	help
		Enable rtl ethernet drivers support.

	select DNS_RESOLVER
	select USB_RTL8150
	select USB_RTL8152
	select CFG80211
	select CFG80211_WEXT
	select MAC80211
	select MAC80211_MESH
	select MAC80211_RC_MINSTREL
	select MAC80211_RC_MINSTREL_HT
	select MAC80211_RC_MINSTREL_VHT

config NETHUNTER_SDR_SUPPORT
	tristate "SDR support"
	default y if NETHUNTER_SUPPORT
	depends on NETHUNTER_SUPPORT
	help
		Enable SDR drivers support like airspy, hackrf and msi2500.

	select MEDIA_SUPPORT
	select MEDIA_SDR_SUPPORT
	select MEDIA_USB_SUPPORT
	select MEDIA_DIGITAL_TV_SUPPORT
	select MEDIA_TUNER
	select MEDIA_TUNER_MSI001
	select MEDIA_TUNER_R820T
	select USB_AIRSPY
	select USB_HACKRF
	select USB_MSI2500
	select DVB_RTL2830
	select DVB_RTL2832
	select DVB_RTL2832_SDR
	select DVB_SI2168
	select DVB_ZD1301_DEMOD

